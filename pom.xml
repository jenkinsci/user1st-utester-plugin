	<developers> 
		<developer> 
			<id>nate-kingsley</id> 
			<name>Nate Kingsley</name> 
			<email>nate@ddblabs.com</email> 
		</developer> 
	</developers> 

        <build>
                <plugins>
			<plugin>
    				<groupId>org.codehaus.mojo</groupId>
    				<artifactId>findbugs-maven-plugin</artifactId>
				<configuration>
					<failOnError>false</failOnError>
				</configuration>
    				<executions>
        				<execution>
            					<phase>none</phase>
        				</execution>
    				</executions>
			</plugin>
                </plugins>
        </build>


